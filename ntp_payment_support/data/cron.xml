<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="recompute_transfer_status" model="ir.cron">
            <field name="name">Payment Support: Recompute Transfer Status</field>
            <field name="model_id" ref="account.model_account_payment" />
            <field name="state">code</field>
            <field name="code">model.task_recompute_transfer_status()</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>

        <record id="find_partner_from_puid" model="ir.cron">
            <field name="name">Payment Support: Find Partner From Puid</field>
            <field name="model_id" ref="account.model_account_bank_statement" />
            <field name="state">code</field>
            <field name="code">model.task_find_partner_from_puid()</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">20</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False" />
        </record>

    </data>

</odoo>