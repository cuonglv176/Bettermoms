<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="wizard_update_move_street_form_view" model="ir.ui.view">
        <field name="name">wizard.update.move.street.form.view</field>
        <field name="model">wizard.update.move.street</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="partner_id" invisible="1"/>
                    <field name="street"/>
                </group>
                <footer>
                    <button name="btn_update_street" string="Update street" type="object" default_focus="1"
                            class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
