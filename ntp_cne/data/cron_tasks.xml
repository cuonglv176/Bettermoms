<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="tax_invoice_sync_cron" model="ir.cron">
            <field name="name">Tax Invoice: Synchronize Tax Invoice From Bizzi</field>
            <field name="model_id" ref="ntp_cne.model_tax_invoice" />
            <field name="interval_number">120</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model.sync_tax_invoice()</field>
        </record>

        <record id="tax_invoice_auto_create_bill_cron" model="ir.cron">
            <field name="name">Tax Invoice: Auto Create Bill Receipt</field>
            <field name="model_id" ref="ntp_cne.model_tax_invoice" />
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model._auto_create_bill_receipts()</field>
        </record>

        <record id="tax_invoice_migrate_many2many" model="ir.cron">
            <field name="name">Tax Invoice: Migrate Many2One to Many2Many Invoice</field>
            <field name="model_id" ref="ntp_cne.model_tax_invoice" />
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model.task_copy_many2one_to_many2many()</field>
            <field name="active" eval="False"/>
        </record>

        <record id="tax_invoice_replace_bill_ref" model="ir.cron">
            <field name="name">Tax Invoice: Replace Bill Ref</field>
            <field name="model_id" ref="account.model_account_move" />
            <field name="interval_number">12</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model.task_replace_text_in_ref()</field>
            <field name="active" eval="False"/>
        </record>

    </data>
</odoo>